{
  "name": "protoforge",
  "description": "SaaS app generator",
  "version": "0.0.1",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "<undefined>"
  },
  "contributors": [],
  "scripts": {
    "deps": "rm -rf ./node_modules ./package-lock.json && npm cache clean --force && npm install",
    "clean": "rm -rf dist .eslintcache .rollup.cache *.run.json",
    "lint": "eslint --cache 'src/**'",
    "lintci": "eslint --output-file .lint.run.json --format json-standard --cache 'src/**'",
    "compile": "tsc --outDir ./dist && npx rollup -c",
    "postcompile": "cp src/client/**/*.* dist/public && rm -rf dist/public/*.tsx dist/client",
    "test": "mocha -r source-map-support/register test/*.test.mjs",
    "testci": "mocha -r source-map-support/register test/*.test.mjs --reporter mocha-reporter-json-standard >| .test.run.json",
    "build": "run-s clean lint compile test",
    "initdb": "psql protoforge -v ON_ERROR_STOP=ON -f ./src/server/db.sql",
    "start": "node dist/server/index.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cuid": "^1.3.1",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/mocha": "^9.1.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.14.41",
    "@types/passport": "^1.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/source-map-support": "^0.5.4",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-formatter-json-standard": "^1.0.1",
    "mocha": "^8.0.1",
    "mocha-reporter-json-standard": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.75.6",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@agyemanjp/http": "^1.0.7",
    "@agyemanjp/somatic": "^3.0.0",
    "@agyemanjp/standard": "^4.1.6",
    "@agyemanjp/storage": "^1.5.2",
    "@aws-sdk/client-s3": "^3.150.0",
    "@danfortsystems/protoforge": "^0.0.18",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "heroku-ssl-redirect": "^0.1.1",
    "js-base64": "^3.6.1",
    "memorystore": "^1.6.6",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "pg-promise": "^10.11.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.0"
  }
}