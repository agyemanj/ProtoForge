{
  "compilerOptions": {
    "target": "es2015",
    "module": "CommonJS",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "es2019",
      "ES2018",
      "es7",
      "es6",
      "es5",
      "dom",
      "es2015.promise",
      "scripthost",
      "dom.iterable",
      "esnext.asynciterable"
    ],
    "jsx": "react",
    "jsxFactory": "createElement",
    "jsxFragmentFactory": "Fragment",
    "keyofStringsOnly": true,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "incremental": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./node_modules"
  ]
}